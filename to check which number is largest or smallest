#include<stdio.h>
int main()
{
int a,b,c;
printf("enter value of a , b and c");
scanf("%d%d%d",&a,&b,&c);
if (a>b && a>c && b>c)
printf("%d%d%d",c,b,a);
if(a>b && a>c && c>b)
printf("%d%d%d",b,c,a);
if(b>a && b>c && a>c)
printf("%d%d%d",b,a,c);
if(b>a && b>c &&  c>a)
printf("%d%d%d",a,c,b);
if(c>a && c>b && a>b)
printf("%d%d%d",b,a,c);
if (c>a && c>b && b>a)
printf("%d%d%d,c",c,b,a);
return 0;
}
