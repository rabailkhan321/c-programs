#include<stdio.h>
int main()
{
    int n1,n2,n3,n4,n5,per;
    printf("enter marks of all your subjects");
    scanf("%d %d %d %d %d",&n1,&n2,&n3,&n4,&n5);
    per=(n1+n2+n3+n4+n5)*100/500;
    if(per>=60)
    {
        printf("first division");
    }
    
    if(per>=50&&per<=59)
    {
        printf("first division");
    }
     if(per>=40&&per<=49)
    {
        printf("second division");
    }
     if(per<40)
    {
        printf("fail");
    }
}
