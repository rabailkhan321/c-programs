#include<stdio.h>

float sum();
float sum()
{
     float a,b;
    float sum=0;
    printf ("enter num 1\n");
    scanf("%f",&a);
    printf ("enter num 2\n");
    scanf("%f",&b);
    sum=a+b;
    printf("the sum is %.2f\n",sum);

}
void main()
{
    sum();
    sum();
    printf("hello world\n");
    sum();
}
